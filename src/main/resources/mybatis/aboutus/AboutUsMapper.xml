<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.aboutus.mapper.AboutUsMapper">

	<select id="getAboutUsInfo" resultType="pd">
		select detail from t_about_us where id = 1
	</select>
	
	<update id="saveAboutUs" parameterType="pd">
		update t_about_us set detail = #{detail} where id = 1
	</update>
</mapper>